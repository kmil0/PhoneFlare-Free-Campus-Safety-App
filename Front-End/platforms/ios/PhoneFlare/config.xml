<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jarvisfilms.phoneflare" version="2.4.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NativeSettings">
        <param name="ios-package" value="NativeSettings" />
    </feature>
    <feature name="NativePageTransitions">
        <param name="ios-package" value="NativePageTransitions" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="Globalization">
        <param name="ios-package" value="CDVGlobalization" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="AppRate">
        <param name="ios-package" value="CDVAppRate" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Contacts">
        <param name="ios-package" value="CDVContacts" />
    </feature>
    <feature name="DatePicker">
        <param name="ios-package" value="DatePicker" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="LongPressFix">
        <param name="ios-package" value="LongPressFix" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <feature name="BluetoothLePlugin">
        <param name="ios-package" value="BluetoothLePlugin" />
    </feature>
    <feature name="LocalNotification">
        <param name="ios-package" onload="true" value="APPLocalNotification" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SocialSharing">
        <param name="ios-package" value="SocialSharing" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Flashlight">
        <param name="ios-package" value="Flashlight" />
    </feature>
    <feature name="Flare">
        <param name="ios-package" value="Flare" />
    </feature>
    <name>PhoneFlare</name>
    <description>
        PhoneFlare texts your friends and family your location when in danger and automatically calls your school's safety dispatch if you're on a supported campus. 
    </description>
    <author email="info@phoneflare.com" href="http://www.phoneflare.com">
      PhoneFlare Dev Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="resources/android/icon/drawable-xhdpi-icon.png" />
    <icon height="57" src="resources/ios/icon/icon.png" width="57" />
